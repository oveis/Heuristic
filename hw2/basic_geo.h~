#ifndef basic_geo_h
#define basic_geo_h
#include <cmath>
struct point {
  int id;
  double x,y,z;
  double dis (point p) {
    return sqrt((x-p.x)*(x-p.x) +
                (y-p.y)*(y-p.y) +
                (z-p.z)*(z-p.z));
  }
};
struct edge {
  int p[2];
  double dis;
  edge(int x,int y, double d) {
    p[0] = x;
    p[1] = y;
    dis = d;
  };
};


#endif
